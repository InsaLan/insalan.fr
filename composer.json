{
    "name": "symfony/framework-standard-edition",
    "license": "MIT",
    "type": "project",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "adoy/oauth2": "^1.3",
        "doctrine/annotations": "^1.2.6",
        "doctrine/dbal": "~2.6",
        "doctrine/doctrine-bundle": "~1.3",
        "doctrine/doctrine-fixtures-bundle": "~2.2",
        "doctrine/doctrine-migrations-bundle": "~1.2",
        "doctrine/migrations": "~1.5",
        "doctrine/orm": "2.6.*",
        "ehesp/steam-login": "^1.1",
        "friendsofsymfony/user-bundle": "^2.2",
        "incenteev/composer-parameter-handler": "~2.0",
        "knplabs/knp-markdown-bundle": "dev-master",
        "lexik/maintenance-bundle": "^2.0",
        "payum/offline": "*@stable",
        "payum/paypal-express-checkout-nvp": "1.4.*",
        "payum/payum-bundle": "*@stable",
        "php-http/guzzle6-adapter": "^1.1",
        "sensio/framework-extra-bundle": "^6.2",
        "sonata-project/admin-bundle": "^3.52",
        "sonata-project/doctrine-orm-admin-bundle": "^3.24",
        "spipu/html2pdf": "^5.2",
        "stof/doctrine-extensions-bundle": "~1.2",
        "symfony/console": "^4.0",
        "symfony/flex": "^1.17",
        "symfony/framework-bundle": "^4.0",
        "symfony/lts": "^4@dev",
        "symfony/monolog-bundle": "^3.5",
        "symfony/requirements-checker": "^2.0",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/twig-bundle": "^4.0",
        "symfony/webpack-encore-bundle": "^1.12",
        "symfony/yaml": "^4.0",
        "twig/extensions": "^1.5.1",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.0",
        "symfony/dotenv": "^4.0",
        "symfony/web-profiler-bundle": "^4.1"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "platform": {
            "php": "7.2.14"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": true
        },
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.5-dev"
        }
    }
}
