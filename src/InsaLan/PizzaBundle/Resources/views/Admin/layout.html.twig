{% extends 'InsaLanAdminBundle::layout.html.twig' %}

{% block stylesheets %}
{{ parent() }}
<style type="text/css">
input[type=submit], input[type=button], button {
    font-size: 0.8em;
    cursor: pointer;
}
td {
    padding: 2px !important;
}
form {
    display: inline;
}
.addInput {
    color: black;
    height: 25px;
    font-size: 18px;
}
.addInput[type=text] {
    height: 21px;
    padding: 2px;
}
</style>
{% endblock %}

{% block main_header %} {{ parent() }} {% endblock %}

{% block rawbody %}{% endblock %}

{% block main_bottom %}{% endblock %}

{% block javascripts %}
    {% javascripts 'bundles/insalan/js/jquery.js' %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
    <script type="text/javascript">
        jQuery.noConflict();
    </script>
    {{ parent() }}
{% endblock %}
